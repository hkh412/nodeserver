body {
    font-family: 'Nanum Gothic', serif;
}

.sub-header {
    font: normal 20px 'Nanum Gothic', serif;
}

.navbar-form {
    margin-top: 0;
}

.dropdown-title {
    margin-right: 3px;
}
.dropdown-frame {
    overflow: auto;
    min-width: 100px;
    max-height: 250px;
}

.footer {
    padding: 20px 0;
    margin-top: 20px;
    border-top: 1px solid #eee;
}

ul.pagination.narrow {
    margin: 10px 0;
}

.bold-text {
    font-weight: bold;
}

.text-wrapper {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}